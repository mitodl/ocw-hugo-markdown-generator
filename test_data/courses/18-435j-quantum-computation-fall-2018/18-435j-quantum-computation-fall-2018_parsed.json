        {
            "bottomtext": "",
            "description": "This section describes the course prerequisites, content, requirements, and grading.",
            "file_location": "https://open-learning-course-data-production.s3.amazonaws.com/18-435j-quantum-computation-fall-2018/4e5a195c0617b08f155b9a40fab906bd_syllabus.html",
